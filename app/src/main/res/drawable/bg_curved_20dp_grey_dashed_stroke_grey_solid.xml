<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#00000000" />
            <stroke
                android:width="1.5dp"
                android:color="#05062D"
                android:dashWidth="5dp"
                android:dashGap="5dp" />
            <padding
                android:bottom="10dp"
                android:left="12dp"
                android:right="12dp"
                android:top="10dp" />
            <corners android:radius="16dp" />
        </shape>
    </item>
</layer-list>